{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "restParams": true,
      "spread": true,
      "experimentalObjectRestSpread": true
    }
  },
  "extends": ["prettier", "eslint:recommended", "plugin:react/recommended"],
  "globals": {
    "fetch": true
  }
}
